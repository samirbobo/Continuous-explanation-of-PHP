{
  "require": {
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.3",
    "doctrine/dbal": "^3.3",
    "doctrine/orm": "^2.11",
    "symfony/cache": "^5.0",
    "doctrine/migrations": "^3.8",
    "twig/twig": "^3.17",
    "twig/intl-extra": "^3.17",
    "slim/slim": "4.*",
    "slim/twig-view": "^3.4",
    "slim/psr7": "^1.7",
    "php-di/php-di": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
