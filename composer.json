{
  "require": {
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.3",
    "psr/container": "^2.0",
    "symfony/mailer": "^6.1",
    "doctrine/dbal": "^3.3",
    "doctrine/orm": "^2.11",
    "symfony/cache": "^6.0",
    "doctrine/migrations": "^3.8",
    "illuminate/database": "^10.48",
    "illuminate/events": "^10.48",
    "guzzlehttp/guzzle": "^7.9",
    "twig/twig": "^3.17",
    "twig/intl-extra": "^3.17"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
